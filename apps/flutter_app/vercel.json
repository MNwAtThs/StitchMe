{
  "version": 2,
  "name": "stitchme-web-app",
  "builds": [
    {
      "src": "pubspec.yaml",
      "use": "vercel-flutter@0.2.0"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "FLUTTER_WEB": "true"
  },
  "functions": {
    "app/**/*": {
      "includeFiles": "web/**"
    }
  }
}
