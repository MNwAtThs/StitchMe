{
  "version": 2,
  "name": "stitchme-monorepo",
  "builds": [
    {
      "src": "apps/flutter_app/pubspec.yaml",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "apps/flutter_app/build/web"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd apps/flutter_app && flutter build web --release"
}
